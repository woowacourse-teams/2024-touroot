{
  "presets": [
    ["@babel/preset-env", { "modules": false }],
    ["@babel/preset-react", { "runtime": "automatic", "importSource": "@emotion/react" }],
    ["@babel/preset-typescript"]
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", { "corejs": 3 }],
    ["@emotion", { "autolabel": "dev-only", "labelFormat": "[local]" }]
  ]
}
